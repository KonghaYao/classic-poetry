---
import Layout from "../../layouts/Layout.astro";
import { SubRoutes } from "../../Home/subRoutes";
import { IndexPage } from "../../poetry/components/BookGenerator/IndexPage";
import "../../poetry/components/BookGenerator/IndexPage.css";
import { CatalogueList } from "../../components/CatalogueList";
export const prerender = true;
export async function getStaticPaths() {
    return SubRoutes.map((i) => {
        return {
            params: {
                book: i.to.slice(1),
            },
            props: {
                name: i.name,
            },
        };
    });
}
const { name } = Astro.props;
---

<Layout title={name + " 中华诗词大典"}>
    <IndexPage title={name}>
        <!-- 首次渲染无数据 -->
        <CatalogueList name={name} client:only />
    </IndexPage>
</Layout>
